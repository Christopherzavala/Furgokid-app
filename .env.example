# ============================================================================
# FIREBASE CONFIGURATION
# ============================================================================
# Obtener estos valores desde Firebase Console:
# https://console.firebase.google.com → Tu Proyecto → Project Settings → General

EXPO_PUBLIC_FIREBASE_API_KEY=your_api_key_here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=1234567890
EXPO_PUBLIC_FIREBASE_APP_ID=1:1234567890:web:abcdef123456

# ============================================================================
# ADMOB CONFIGURATION
# ============================================================================
# Obtener desde AdMob Console: https://admob.google.com

ADMOB_ANDROID_APP_ID=ca-app-pub-xxxxxxxx~yyyyyyyy
ADMOB_IOS_APP_ID=ca-app-pub-xxxxxxxx~zzzzzzzz

BANNER_AD_UNIT_ID=ca-app-pub-xxxxxxxx/yyyyyyyy
INTERSTITIAL_AD_UNIT_ID=ca-app-pub-xxxxxxxx/yyyyyyyy
REWARDED_AD_UNIT_ID=ca-app-pub-xxxxxxxx/yyyyyyyy

BANNER_AD_UNIT_IOS=ca-app-pub-xxxxxxxx/yyyyyyyy
INTERSTITIAL_AD_UNIT_IOS=ca-app-pub-xxxxxxxx/yyyyyyyy
REWARDED_AD_UNIT_IOS=ca-app-pub-xxxxxxxx/yyyyyyyy

# ============================================================================
# GOOGLE MAPS API
# ============================================================================
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# ============================================================================
# APP CONFIGURATION
# ============================================================================
NODE_ENV=development
EXPO_DEBUG=true
